@model IEnumerable<DataLibrary.Models.DatabaseList>


@{
    ViewBag.Title = "LoadDatabases";
}
<header>
    <hr />
</header>
<h2>Orders List</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Create_date)
        </th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Create_date)
    </td>
    <td>
    @Html.ActionLink("View Order", "OrderForPrint", new { name = item.Name })
        </td>
</tr>
    }

</table>
